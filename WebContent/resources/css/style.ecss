/*default style for buttons in entire application*/
button, 
input[type="reset"], 
input[type="submit"], 
input[type="button"] {
	min-width: 100px;
	padding: 0 10px;
	min-height: 20px;
	outline-width: 0px;
	margin: 5px;
	border-radius: 8px;
	border-style: solid;
}

input[type="text"]:disabled,textarea:disabled{
		color:blue;
}

.outputTextDisabled{
	color:blue;
}

input[type="text"]{
	background-position: 0px 1.5px !important;
}


select:disabled {
    color:blue;
}

.btn125 {
	width: 125px;
}

.btn150 {
	width: 150px;
}

.btn175 {
	width: 175px;
}

.btn200 {
	width: 200px;
}

.hoverButton {
	padding:4px;
	position:relative;
}

.hoverButton:hover {
	padding:2px;
	width:20px;
	height:20px;
	top:0px;
	left:0px;
}

/*Controls*/
input[type=text], 
input[type=password], 
textarea {
	border-style: solid;
	border-width: 1px;
	outline-width: 0px;
	height: 20px;
	padding: 0 7px;
	vertical-align: middle;
	border-radius: 8px;
	margin: 5px;
}

fieldset {
	padding: 0 7px;
	vertical-align: middle;
	border-radius: 8px;
	margin: 5px;
}

/*big check box*/
input.bigcheck[type=checkbox]
{
width:18px;
height:18px;
}

textarea {
	padding-right: 0;
}

select {
	border-style: solid;
	border-width: 1px;
	height: 22px;
	vertical-align: middle;
	border-radius: 8px 0 0 8px;
	padding: 0 0 0 7px;
	min-width: 100px;
	margin: 5px;
}

.toolButton {
	background: none !important;
	width: 16px;
	height: 16px;
	padding: 0px;
	border: 0px;
	outline: 0px;
	margin: 5px;
	cursor: pointer;
}

.toolButton:HOVER {
	background: none;
	padding: 0px;
	border: 0px;
	outline: 0px;
	margin: 1px;
	width: 18px;
	height: 18px;
	cursor: pointer;
}

.toolButton[disabled] {
	background: none;
	padding: 0px;
	border: 0px;
	outline: 0px;
	margin: 4px;
	width: 12px;
	height: 12px;
	cursor: default;
}

