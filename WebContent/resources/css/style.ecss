/*search*/

.searchRowContainer {
	display:block; 
	padding: 5px 0; 
	border-bottom:solid 1px;
	border-color: '#{richSkin.panelBorderColor}';
}

.searchRowContainerCell{
	display:inline-block;
}

.searchButtonsCell{
	float:right;
}

.searchRowContainer:nth-of-type(odd),
.searchTable tr:nth-of-type(odd) {
	background-color: '#{richSkin.generalBackgroundColor}';
}

.searchRowContainer:nth-of-type(even),
.searchTable tr:nth-of-type(even) {
	background-color: '#{richSkin.tableBackgroundColor}';
}

.active-row {
	background-color: #FFEBDA !important;
}

.selected-row {
	background-color: #7F7F7F !important;
}

.searchTable tr th:last-of-type {
	border-radius:0 8px 0 0;
}

.searchTable tr th:first-of-type {
	border-radius:8px 0 0 0;
}

.searchTableAddCriteria,  .searchTableAddCriteria tr td{
	font-size: 11px;
  	padding: 0px;
 	border-collapse: collapse;
  	padding: 0px;
}

.searchTableAddCriteria tr td select {
	height:18px;
}


.searchTable, .searchTable tr td {
	border-top:none;
	border-left:none;
	border-right:none;
	font-size:11px;
	padding:3px;
}

.searchTable tr th, .searchTable tfoot tr td, .searchTable  {
	border:none !important;
	text-align:left;
}

.searchTable tfoot tr td:first-of-type{
	border-radius:0 0 0 8px;
}	

.searchTable tfoot tr td:last-of-type{
	border-radius:0 0 8px 0;
}

.searchTable thead tr,
.searchTable tfoot tr {
	background-color: transparent !important;
}

.searchDataScroller {
	display:block !important; 
	text-align: center; 
	padding:20px 0 !important;
}

.searchPanel {
	width:100%;
	display:inline-block;	
/*	border-top:solid 1px;*/
	border-color: '#{richSkin.panelBorderColor}';
}

.searchTable{
	width:100%;
}
